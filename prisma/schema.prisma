// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./prisma/database.db"
}

generator client {
  provider = "prisma-client-js"
}

model Images {
  id       Int    @id @default(autoincrement())
  src      String
  title    String
  location String
}
